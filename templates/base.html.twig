        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-dark navbar-bg mb-5">
<a style="margin-left: 0.1rem;" class="navbar-brand space-brand" href="#">
                    My Great Website !
</a>
<ul> <li>
<a href="{{ path('student_index') }}">All Students</a> </li>
</ul> </nav>

          <div class="container">
        <div class="row">
            <div class="col-sm-12">
            {% block body %}{% endblock %}
